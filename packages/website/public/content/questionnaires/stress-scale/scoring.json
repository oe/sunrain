{
  "scoringRules": [
    {
      "id": "total_score",
      "name": "Perceived Stress Scale Total Score",
      "description": "Sum of all item scores (reverse scoring for items 4, 5, 7, 8)",
      "calculation": "custom",
      "questionIds": ["stress-1", "stress-2", "stress-3", "stress-4", "stress-5", "stress-6", "stress-7", "stress-8", "stress-9", "stress-10"],
      "customFormula": "stress-1 + stress-2 + stress-3 + (4-stress-4) + (4-stress-5) + stress-6 + (4-stress-7) + (4-stress-8) + stress-9 + stress-10",
      "ranges": [
        {
          "min": 0,
          "max": 13,
          "label": "Low Stress",
          "description": "Low perceived stress levels",
          "riskLevel": "low"
        },
        {
          "min": 14,
          "max": 26,
          "label": "Moderate Stress",
          "description": "Moderate perceived stress levels",
          "riskLevel": "medium"
        },
        {
          "min": 27,
          "max": 40,
          "label": "High Stress",
          "description": "High perceived stress levels",
          "riskLevel": "high"
        }
      ]
    }
  ]
}
