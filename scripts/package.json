{
  "name": "mental-health-content-fetcher",
  "version": "1.0.0",
  "description": "Automated content fetching system for mental health resources",
  "type": "module",
  "main": "content-fetcher/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch content-fetcher/index.ts",
    "fetch:books": "dotenv -e .env -- tsx content-fetcher/cli.ts --type books",
    "fetch:movies": "dotenv -e .env -- tsx content-fetcher/cli.ts --type movies",
    "fetch:music": "dotenv -e .env -- tsx content-fetcher/cli.ts --type music",
    "fetch:all": "dotenv -e .env -- tsx content-fetcher/cli.ts --type all",
    "test:validator": "tsx content-fetcher/test-validator.ts",
    "i18n:sync": "tsx content-fetcher/i18n-cli.ts --action sync",
    "i18n:detect": "tsx content-fetcher/i18n-cli.ts --action detect",
    "i18n:report": "tsx content-fetcher/i18n-cli.ts --action report",
    "i18n:cleanup": "tsx content-fetcher/i18n-cli.ts --action cleanup"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "dotenv-cli": "^9.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}