{
  "name": "sunrain",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:website": "turbo run dev --filter=@sunrain/website",
    "dev:content-fetcher": "turbo run dev --filter=@sunrain/content-fetcher",
    "dev:shared": "turbo run dev --filter=@sunrain/shared",
    "build": "turbo run build",
    "build:website": "turbo run build --filter=@sunrain/website",
    "build:content-fetcher": "turbo run build --filter=@sunrain/content-fetcher",
    "build:shared": "turbo run build --filter=@sunrain/shared",
    "build:prod": "turbo run clean && turbo run build",
    "preview": "turbo run preview",
    "preview:website": "turbo run preview --filter=@sunrain/website",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint --continue",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "clean:all": "turbo run clean && rm -rf node_modules/.cache",
    "fetch:books": "turbo run fetch:books --filter=@sunrain/content-fetcher",
    "fetch:movies": "turbo run fetch:movies --filter=@sunrain/content-fetcher",
    "fetch:music": "turbo run fetch:music --filter=@sunrain/content-fetcher",
    "fetch:all": "turbo run fetch:all --filter=@sunrain/content-fetcher",
    "i18n:sync": "turbo run i18n:sync --filter=@sunrain/content-fetcher",
    "i18n:detect": "turbo run i18n:detect --filter=@sunrain/content-fetcher",
    "i18n:report": "turbo run i18n:report --filter=@sunrain/content-fetcher",
    "i18n:cleanup": "turbo run i18n:cleanup --filter=@sunrain/content-fetcher",
    "test:validator": "turbo run test:validator --filter=@sunrain/content-fetcher"
  },
  "dependencies": {
    "@sunrain/shared": "workspace:*"
  },
  "devDependencies": {
    "turbo": "^2.3.4"
  }
}